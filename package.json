{
  "name": "openwithcode",
  "displayName": "OpenWithCode",
  "description": "VSCode extension which adds 'Open in New Window' option to the right-click context menu in the Explorer.",
  "icon": "media/icon.png",
  "publisher": "kevinchatham",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinchatham/VSCode-OpenWithCode"
  },
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "when": "resourceScheme == file",
          "command": "extension.openWithCode"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.openWithCode",
        "title": "Open in New Window"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
